}

// IP 范围类型的 Target
{
  "type": "ip_range",
  "value": "192.168.1.0/24",
  "source": "file:/path/to/targets.txt",
  "meta": {
    "network": {
      "cidr": "192.168.1.0/24",
      "location": "Beijing",
      "subnet_mask": "255.255.255.0",
      "gateway": "192.168.1.1"
    }
  }
}

