{
  "name": "Service Rules",
  "version": "1.0",
  "type": "service",
  "samples": [
    {
      "match_str": "(?i)^SSH-[\\d\\.]+-OpenSSH_([\\w\\.]+)",
      "vendor": "openbsd",
      "product": "openssh",
      "part": "a",
      "cpe": "cpe:2.3:a:openbsd:openssh:$1:*:*:*:*:*:*:*"
    },
    {
      "match_str": "(?i)Apache/([\\d\\.]+)",
      "vendor": "apache",
      "product": "http_server",
      "part": "a",
      "cpe": "cpe:2.3:a:apache:http_server:$1:*:*:*:*:*:*:*"
    },
    {
      "match_str": "(?i)nginx/([\\d\\.]+)",
      "vendor": "f5",
      "product": "nginx",
      "part": "a",
      "cpe": "cpe:2.3:a:f5:nginx:$1:*:*:*:*:*:*:*"
    }
  ]
}